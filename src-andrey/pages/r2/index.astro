---
import BaseLayout from '../../layouts/BaseLayout.astro';
---
<BaseLayout title="R2 Objects">
  <p class="mb-4">Listing first 20 objects from R2 bucket (MEDIA).</p>
  <pre id="out" class="text-xs bg-gray-100 p-3"></pre>
  <script>
    fetch('/r2/list').then(r=>r.json()).then(d=>{
      document.getElementById('out').textContent = JSON.stringify(d,null,2);
    });
  </script>
</BaseLayout>
